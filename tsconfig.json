{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "outDir": "dist",
        "sourceMap": true,
        "lib": [
            "es6",
            "dom"
        ],
        "types": [
            "inversify",
            "inversify-express-utils",
            "mocha",
            "reflect-metadata"
        ],
        "noImplicitAny": false,
        "emitDecoratorMetadata": true,
        "experimentalDecorators": true,
        "moduleResolution": "node"
    },
    "compileOnSave": false
}
